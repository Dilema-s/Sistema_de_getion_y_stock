	<queryString>
		<![CDATA[SELECT
     v.`id_venta_producto`,
     p.`id_producto`,
     p.`nombre`,
     p.`precio`,
     p.`descripcion`,
     sold.`fecha`,
     sold.`total`,
     sold.`id_venta`,
     c.`apellido`,
     c.`direccion`,
     c.`dni`
FROM
     `product` p INNER JOIN `venta_producto` v ON p.`id_producto` = v.`id_producto`
     INNER JOIN `venta` sold ON v.`id_venta` = sold.`id_venta`
     INNER JOIN `cliente` c ON sold.`cliente` = c.`id_cliente`
WHERE
     v.id_venta = 44]]>
	</queryString>
